<section id='prof-info'>
  <h2>Professor Information</h2>
  <p>Name: <%= @professor.name %></p>
  <p>Age: <%= @professor.age %></p>
  <p>Specialty: <%= @professor.specialty %></p>
</section>
<br>
<section id='prof-students'>
  <h2>Students</h2>
  <p id='avg-student-age'><%= @professor.average_student_age %></p>
  <% @professor.students.each do |student| %>
    <p>Name: <%= student.name %></p>
  <% end %>
</section>
